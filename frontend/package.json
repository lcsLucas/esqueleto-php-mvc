{
  "name": "sites_monkey",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass --output-style expanded css/scss/estilo.scss public/css/estilo.css",
    "scssmin": "node-sass --output-style compressed css/scss/estilo.scss public/css/estilo.min.css",
    "prefixercss": "npx postcss css/*.css --use autoprefixer -d css/",
    "watch:scss": "onchange --await-write-finish 1500 -i -k \"css/scss/**/*.scss\" -- npm run scss",
    "watch:js": "onchange --await-write-finish 1500 -i -k \"js/sources/*.js\" -- npm run build:prod",
    "dist": "npm run scss && npm run scssmin && prefixercss",
    "build:watch": "webpack -w --mode development",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "start:dev": "webpack-dev-server"
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^2.3.4",
    "uglify-js": "^3.6.9",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-fix-style-only-entries": "^0.4.0"
  },
