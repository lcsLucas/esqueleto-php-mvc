    "axios": "^0.19.2",
    "bootstrap": "^4.3.1",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "jquery": "^3.4.1",
    "jquery-validation": "^1.19.3",
    "perfect-scrollbar": "^1.5.0",
    "popper.js": "^1.16.0",
    "sweetalert2": "^9.15.2"
  }
}
